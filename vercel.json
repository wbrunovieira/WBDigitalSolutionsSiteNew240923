{
  "redirects": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1",
      "permanent": false
    },
    {
      "source": "/(.*)",
      "destination": "https://wbdigitalsolutions.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.wbdigitalsolutions.com"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' https:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https:; object-src 'none'; frame-src 'self' https://www.youtube.com;"
        }
      ]
    }
  ]
}
