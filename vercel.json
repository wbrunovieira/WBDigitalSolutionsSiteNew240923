{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://wbdigitalsolutions.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.wbdigitalsolutions.com"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; frame-ancestors 'self';"
        }
       
      ]
    }
  ]
}
